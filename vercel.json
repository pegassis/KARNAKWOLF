{
  "routes": [
    {
      "src": "^/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^(?!.*\\.(js|css|png|jpg|jpeg|svg|ico|webp|gif)$).*",
      "dest": "/index.html",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ]
}